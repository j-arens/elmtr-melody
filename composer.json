{
    "name": "j-arens/elmtr-melody",
    "type": "Elementor widget",
    "require": {
        "downshiftorg/wp-router": "~3.0",
        "downshiftorg/container": "~2.0"
    },
    "require-dev": {
        "kahlan/kahlan": "^4.1",
        "eloquent/phony-kahlan": "^1.0"
    },
    "authors": [
        {
            "name": "Josh Arens",
            "email": "arens.joshua@sbcglobal.net"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Melody\\": "./plugin/"
        },
        "files": [
            "./plugin/Core/functions.php",
            "./plugin/assets/wordpressEnqueueChunksPlugin.php"
        ]
    },
    "scripts": {
        "test": "kahlan --config=dev/kahlan/config.php --src=plugin --reporter=verbose"
    }
}
